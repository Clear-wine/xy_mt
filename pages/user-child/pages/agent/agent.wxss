page {
  padding: 0;
  margin: 0;
}

.agent-top {
  position: relative;
  overflow: hidden;
}

.agent-banner {
  width: 100%;
  overflow: hidden;
}

.agent-banner image {
  width: 750rpx;
  height: 538rpx;
  /* background-image: linear-gradient(90deg, #ff9d1b 0%, #ffb62a 100%); */
}

/* 改版壹叁代理商 */

.cen-wrap {
  position: absolute;
  top: 17%;
  width: 100%;
  color: #fff;
}

.cen-wrap .withdraw-view {
  text-align: center;
  font-size: 33rpx;
}

.withdraw-view .price {
  font-size: 50rpx;
}

.cen-wrap .row-price {
  display: flex;
  justify-content: space-around;
  margin-top: 6%;
}

.col-price-view {
  text-align: center;
  font-size: 26rpx;
}

.withdraw-btn {
  background-color: #fff;
  position: absolute;
  right: 0;
  top: 22%;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  width: 125rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  color: #ff9736;
  font: 28rpx;
}

.agent-wrap {
  width: 95%;
  overflow: hidden;
  position: relative;
  top: -195rpx;
  margin: auto;
}

.li-view {
  background-color: #fff;
  width: 100%;
  /* display: flex;
  justify-content: center;
  align-items: center; */
  border-radius: 20rpx;
  padding: 4% 0;
  margin-bottom: 3%;
}

.li-view .team-view {
  text-align: center;
}

.row-team>text {
  font-size: 34rpx;
  padding: 0 2%;
  position: relative;
  top: -5rpx;
}

.team-view .teamimg {
  width: 36rpx;
  height: 32rpx;
}

.team-view .team-more {
  width: 28rpx;
  height: 26rpx;
  position: relative;
  top: -4rpx;
}

.team-view .teamNum {
  padding-top: 2%;
}

.team-view .teamNum>text {
  font-size: 40rpx;
  font-family: PingFang-SC-Bold;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #353b56;
}

.li-view .row-num {
  display: flex;
  justify-content: space-around;
}

.row-num .col-num {
  text-align: center;
}

.col-num .title {
  font-family: PingFang-SC-Medium;
  font-size: 28rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #999;
  padding-bottom: 8%;
}

.col-num .module>text {
  font-family: PingFang-SC-Bold;
  font-size: 43rpx;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #353b56;
}
