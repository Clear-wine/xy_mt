<!-- 我的团队 -->
<view class="agent-top">
  <view class="agent-banner">
    <image src="../images/agent_bg.png"></image>
  </view>
  <view class="cen-wrap">
    <view class="withdraw-view">
      <view class="price">{{data.withdrawableAmount}}</view>
      <view class="title">可提现金额(元)</view>
    </view>
    <view class="row-price">
      <view class="col-price-view" bindtap="handleToShare">
        <view class="price">{{data.consumeAmount}}</view>
        <view class="title">历史累计分润(元)</view>
      </view>
      <view class="col-price-view" bindtap="handleNoToShare">
        <view class="price">{{data.waitAmount}}</view>
        <view class="title">未入账的分润(元)</view>
      </view>
    </view>
  </view>
  <view class="withdraw-btn" bindtap='handleDrawings'>提现</view>
