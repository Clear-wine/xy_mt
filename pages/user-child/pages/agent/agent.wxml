<!-- 代理说明 -->
<view class="agent-wrap">
  <view class="li-view">
    <view class="team-view" bindtap="handleToTeam">
      <view class="row-team">
        <image class="teamimg" src="../images/my_team.png"></image>
        <text>我的团队</text>
        <image class="team-more" src="../images/my_team_enter.png"></image>
      </view>
      <view class="teamNum">
        <text>{{data.teamNum}}</text> 人</view>
    </view>
  </view>
  <view class="li-view">
    <view class="row-num">
      <view class="col-num">
        <view class="title">总分润商品数</view>
        <view class="module">
          <text>{{data.teamTradeCount}}</text> 件</view>
      </view>
      <view class="col-num">
        <view class="title">团队总交易额</view>
        <view class="module">
          <text>{{data.teamTradeAmount}}</text>元 </view>
      </view>
    </view>
  </view>
</view>


