/*隐藏滚动条*/

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.box-container {
  display: inline-block;
  position: relative;
}

.nav-name {
  display: inline-block;
  font-size: 28rpx;
  color: #fff;
  border-bottom: 2px solid transparent;
  padding: 10px;
}

.nav-hover {
  font-size: 34rpx;
  font-weight: bold;
}

.nav-hover::after {
  content: '';
  position: absolute;
  left: 22rpx;
  top: auto;
  bottom: 0;
  right: auto;
  height: 6rpx;
  width: 55rpx;
  border-radius: 20rpx;
  background-color: #fff;
}

