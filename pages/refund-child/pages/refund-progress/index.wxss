  width: 280rpx;
}
.center-er-san{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:bold;
}
/*进度到达的颜色*/
.yescolor{
  color:rgba(255,125,38,1);
}
/*进度未到达的颜色*/
.nocolor{
  color:rgba(197,197,197,1);
}
.center-san{
  width:650rpx;
  height:0rpx;
