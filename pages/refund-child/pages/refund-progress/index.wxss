  margin-top: 8rpx;
}
.top-yi{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,125,38,1);
  padding-top: 12rpx;
  margin-left: 24rpx;
}
.top-er{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(153,153,153,1);
  margin-left: 24rpx;
}
.center{
  width:702rpx;
  height:308rpx;
  background:rgba(255,255,255,1);
